GEMINI_MODEL=gemini-1.5-flash

# SERVIDOR
PORT=3000
NODE_ENV=development

# MERCADO PAGO (opcional)
MERCADOPAGO_ACCESS_TOKEN=
MERCADOPAGO_PUBLIC_KEY=

# WHATSAPP (opcional - Meta Business API)
